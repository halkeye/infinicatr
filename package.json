    "node-sass": "^4.7.2",
    "prettier-eslint-cli": "^4.7.1",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.0",
    "surge": "^0.20.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.0",
