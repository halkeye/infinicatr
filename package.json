{
  "name": "infinicatr",
  "version": "2.0.0",
  "description": "Endless Supply of cat pictures (as provided by flickr)",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint app server.dev.js webpack.config.js",
    "test": "npm run lint; npm run build",
    "dev": "node ./server.dev.js",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --env.prod=true",
    "build": "cross-env NODE_ENV=production webpack --env.prod=true",
    "postversion": "git push --tags && conventional-github-releaser -p angular",
    "build_chrome_package": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack -p && cd dist && zip -r ../infinicatr.zip *",
    "deploy:surge": "rm -rf dist && npm run build && surge -p dist -d infinicatr.surge.sh",
    "deploy:apps": "rm -rf dist && npm run build && cd dist && rsync -vazt --delete . rib:/home/apache/vhosts/gavinmogan.com/apps/infinicatr/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/halkeye/infinicatr.git"
  },
  "author": "Gavin Mogan <gavin@kodekoan.com>",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "object-assign": "^4.1.0",
    "url-search-params": "^0.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.0",
    "bootstrap-sass": "^3.3.5",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.14.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^4.0.0",
    "json-loader": "^0.5.2",
    "loader-utils": "^1.0.0",
    "loaders.css": "^0.1.1",
    "mousetrap": "^1.5.3",
    "mustache-loader": "^1.0.0",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.19.0",
    "surge": "^0.19.0",
    "url-loader": "^0.6.1",
    "web-app-manifest-loader": "^0.1.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.2",
    "worker-loader": "^1.0.0"
  }
}
