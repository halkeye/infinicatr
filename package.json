{
  "name": "infinicatr",
  "version": "2.0.0",
  "description": "Endless Supply of cat pictures (as provided by flickr)",
  "scripts": {
    "start": "node app-server.js",
    "lint": "eslint app server.dev.js webpack.config.js",
    "test": "npm run lint; npm run build",
    "dev": "node ./server.dev.js",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "build_chrome_package": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack -p && cd dist && zip -r ../infinicatr.zip *",
    "deploy:surge": "rm -rf dist && npm run build && surge -p dist -d infinicatr.surge.sh",
    "deploy:apps": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack -p && cd dist && rsync -vazt --delete * rib:/home/apache/vhosts/gavinmogan.com/apps/infinicatr/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/halkeye/infinicatr.git"
  },
  "author": "Gavin Mogan <gavin@kodekoan.com>",
  "dependencies": {
    "autoprefixer-core": "^6.0.1",
    "bluebird": "^3.4.1",
    "cfenv": "1.0.x",
    "es6-promise": "^4.0.3",
    "express": "4.14.1",
    "isomorphic-fetch": "^2.2.1",
    "object-assign": "^4.1.0",
    "url-search-params": "^0.7.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.13.2",
    "bootstrap-sass": "^3.3.5",
    "consolelog": "^2.1.3",
    "css-loader": "^0.28.0",
    "del": "^2.2.1",
    "es5-shim": "^4.1.7",
    "es6-shim": "^0.35.1",
    "eslint": "^3.2.2",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.22.0",
