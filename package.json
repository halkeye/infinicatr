{
  "name": "infinicatr",
  "version": "2.0.0",
  "description": "Endless Supply of cat pictures (as provided by flickr)",
  "license": "MIT",
  "scripts": {
    "format": "prettier-eslint \"app/**/*.js\"",
    "start": "webpack serve --config webpack.dev.js --mode=development",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "test": "npm run lint; npm run build",
    "dev": "webpack serve --config webpack.dev.js --mode=development",
    "prod": "webpack --config webpack.prod.js --mode=production",
    "build": "webpack --config webpack.dev.js --mode=development",
    "postversion": "git push --tags && conventional-github-releaser -p angular",
    "build_chrome_package": "rm -rf dist && npm run build && cd dist && zip -r ../infinicatr.zip *",
    "deploy:surge": "rm -rf dist && npm run build && surge -p dist -d infinicatr.surge.sh",
    "deploy:apps": "rm -rf dist && npm run build && cd dist && rsync -vazt --delete . rib:/home/apache/vhosts/gavinmogan.com/apps/infinicatr/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/halkeye/infinicatr.git"
  },
  "author": "Gavin Mogan <gavin@kodekoan.com>",
