{
  "name": "infinicatr",
  "version": "2.0.0",
  "description": "Endless Supply of cat pictures (as provided by flickr)",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve dist",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "test": "npm run lint; npm run build",
    "build_chrome_package": "rm -rf dist && npm run build && cd dist && zip -r ../infinicatr.zip *",
    "deploy:surge": "rm -rf dist && npm run build && surge -p dist -d infinicatr.surge.sh"
  },
  "engines": {
    "node": ">=14 <15",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/halkeye/infinicatr.git"
  },
  "author": "Gavin Mogan <gavin@kodekoan.com>",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "es6-promise": "^4.2.8",
    "object-assign": "^4.1.1",
    "url-search-params": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@web/rollup-plugin-html": "^1.8.0",
    "babel-eslint": "^10.1.0",
    "bootstrap-sass": "^3.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "extend": "^3.0.2",
    "loaders.css": "^0.1.2",
    "mini-css-extract-plugin": "^2.0.0",
    "mousetrap": "^1.6.5",
    "node-libs-browser": "^2.2.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "rollup": "^2.52.6",
    "rollup-plugin-mustache": "^0.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^12.0.0",
    "surge": "^0.23.0"
  }
}
