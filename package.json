{
  "name": "infinicatr",
  "version": "2.0.0",
  "description": "Endless Supply of cat pictures (as provided by flickr)",
  "scripts": {
    "start": "node app-server.js",
    "upload_bluemix": "npm run build && cf push infinicatr",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "build_chrome_package": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack -p && cd dist && zip -r ../infinicatr.zip *",
    "deploy_apps": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack -p && cd dist && rsync -vazt --delete * rib:/home/apache/vhosts/gavinmogan.com/apps/infinicatr/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/halkeye/infinicatr.git"
  },
  "author": "Gavin Mogan <gavin@kodekoan.com>",
