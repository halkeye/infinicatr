{
  "name": "infinicatr",
  "version": "2.0.0",
  "description": "Endless Supply of cat pictures (as provided by flickr)",
  "license": "MIT",
  "scripts": {
    "format": "prettier-eslint \"app/**/*.js\"",
    "start": "webpack-dev-server --config webpack.dev.js --mode=development",
    "lint": "eslint app server.dev.js webpack.config.js",
    "test": "npm run lint; npm run build",
    "dev": "webpack-dev-server --config webpack.dev.js --mode=development",
    "prod": "webpack-dev-server --config webpack.prod.js --mode=production",
    "build": "webpack --config webpack.dev.js --mode=production",
    "postversion": "git push --tags && conventional-github-releaser -p angular",
    "build_chrome_package": "rm -rf dist && npm run build && cd dist && zip -r ../infinicatr.zip *",
    "deploy:surge": "rm -rf dist && npm run build && surge -p dist -d infinicatr.surge.sh",
    "deploy:apps": "rm -rf dist && npm run build && cd dist && rsync -vazt --delete . rib:/home/apache/vhosts/gavinmogan.com/apps/infinicatr/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/halkeye/infinicatr.git"
  },
  "author": "Gavin Mogan <gavin@kodekoan.com>",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "object-assign": "^4.1.1",
    "url-search-params": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.0",
    "eslint": "^5.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "extend": "^3.0.1",
    "file-loader": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "image-webpack-loader": "^4.1.0",
